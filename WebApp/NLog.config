<?xml version="1.0" encoding="utf-8" ?>
<nlog internalLogFile="nlog.txt" internalLogLevel="debug" xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 <extensions> 
    <add assembly="NLogViewer"/> 
  <add assembly="NLog.MSFTTeams" />
  </extensions> 
  <targets async="true">
    <target
      xsi:type="CacheTarget"
      name="cache"/>
  </targets>
  
    <targets>
 <target xsi:type="MSFTTeamsTarget" name="msftTeams" layout="${longdate} ${uppercase:${level}} ${message}" CardTitle="User: "  TextMarkdown="##### InstallationId: " ThemeColorError="FF0000" ThemeColorDefault="" WebHookUrl="https://outlook.office.com/webhook/d3e8f9d8-c43c-4a0b-a2ca-2f7c73a422ea@3956be4f-5b3e-40ee-a904-74d46f3f1b7f/IncomingWebhook/14102b6faab4473fafc8eeb427f62df4/07763d67-a182-473a-be2b-2fa3762eea30"/>
<target xsi:type="Chainsaw"
          name="chainsaw"
          newLine="false"
          layout="${threadid}-${threadname} ${longdate}|${level:uppercase=true}|${logger}|${message}"
          encoding="utf-8"
          address="udp://127.0.0.1:4445"/>
<target xsi:type="Chainsaw"
          name="chainsaw_serenity"
          newLine="false"
          layout="${threadid}-${threadname} ${longdate}|${level:uppercase=true}|${logger}|${message}"
          encoding="utf-8"
          address="udp://serenity:4445"/>
<target name="logfile" xsi:type="File"  fileName="file.txt"  layout="${threadid}-${threadname} ${longdate}|${level:uppercase=true}|${logger}|${message}" />
<target name="logfile" xsi:type="File"  fileName="file.xml"  layout="${log4jxmlevent}"/>
        <target error="true" name="logconsole" xsi:type="Console" layout="${threadid}-${threadname} ${longdate}|${level:uppercase=true}|${logger}|${message}"/>
  <target xsi:type="CacheTarget" name="cache1"/>
        <target xsi:type="NLogViewer"
        name="viewer"
        address="udp://10.25.0.102:9999"
	includeSourceInfo="true"
	includeCallSite="true"/>
	
        <target xsi:type="NLogViewer"
        name="viewer3"
        address="udp://10.25.0.102:878"/>
        <target xsi:type="NLogViewer"
                name="viewer2"
                address="udp://10.25.0.102:9998"/>
    </targets>

    <rules>
        <logger name="*" minlevel="Info" writeTo="logconsole" />
        <logger name="*" minlevel="Debug" writeTo="logfile" />
        <logger name="*"
                minlevel="Trace"
                writeTo="viewer" />
        <logger name="*"
                minlevel="Debug"
                writeTo="chainsaw" />
        <logger name="*"
                minlevel="Trace"
                writeTo="chainsaw_serenity" />
        <logger name="*"
                minlevel="Trace"
                writeTo="viewer3" />
        <logger name="*"
                minlevel="Info"
                writeTo="viewer2" />
        <logger name="*"
                minlevel="Debug"
                writeTo="cache1" />
        <logger name="*"
                minlevel="Warn"
                writeTo="msftTeams" />
    <logger name="*" writeTo="cache" minlevel="Debug"/> 
    </rules>
</nlog>
