<Window x:Class="ParseLogs.ConsoleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParseLogs"
        xmlns:wpf="clr-namespace:ConsoleControl.WPF;assembly=ConsoleControl.WPF"
        mc:Ignorable="d"
        Title="ConsoleWindow" Height="450" Width="800">
    <Grid>
        <wpf:ConsoleControl Name="ConsoleControl"><wpf:ConsoleControl.Background><SolidColorBrush Color="Black"/></wpf:ConsoleControl.Background>
            <wpf:ConsoleControl.Foreground><SolidColorBrush Color="LawnGreen"></SolidColorBrush></wpf:ConsoleControl.Foreground>
            </wpf:ConsoleControl>
    </Grid>
</Window>
