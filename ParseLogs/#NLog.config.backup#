        <target xsi:type="NLogViewer"
                name="viewer2"
                address="udp://10.25.0.102:9998"/>
    </targets>

    <rules>
        <logger name="*" minlevel="Info" writeTo="logconsole" />
        <logger name="*" minlevel="Debug" writeTo="logfile" />
        <logger name="*"
                minlevel="Trace"
                writeTo="viewer" />
        <logger name="*"
                minlevel="Debug"
                writeTo="chainsaw" />
        <logger name="*"
